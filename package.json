{
  "name": "react-template",
  "version": "1.0.0",
  "description": "Get started with React, Redux, and React-Router!",
  "main": "index.js",
  "engines": {
    "node": ">=4.5.0",
    "npm": "^3.0.0"
  },
  "scripts": {
    "clean": "rimraf dist",
    "compile": "better-npm-run compile",
    "lint": "eslint bin build config server src tests",
    "lint:fix": "npm run lint -- --fix",
    "deploy": "better-npm-run deploy",
    "dev": "better-npm-run deploy:dev",
    "prod": "better-npm-run deploy:prod",
    "start": "better-npm-run start"
  },
  "betterScripts": {
    "compile": {
      "command": "node bin/compile",
      "env": {
        "DEBUG": "app:*"
      }
    },
    "dev": {
      "command": "nodemon bin/server --ignore dist --ignore coverage --ignore tests --ignore src",
      "env": {
        "NODE_ENV": "development",
        "DEBUG": "app:*"
      }
    },
    "deploy": {
      "command": "npm run clean && npm run compile",
      "env": {
        "DEBUG": "app:*"
      }
    },
    "deploy:dev": {
      "command": "npm run deploy",
      "env": {
        "NODE_ENV": "development",
        "DEBUG": "app:*"
      }
    },
    "deploy:prod": {
      "command": "npm run deploy",
      "env": {
        "NODE_ENV": "production",
        "DEBUG": "app:*"
      }
    },
    "start": {
      "command": "webpack-dev-server --inline --progress --config bin/start",
      "env": {
        "NODE_ENV": "development",
        "DEBUG": "app:*"
      }
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/bodyno/react-starter-kit.git"
  },
  "author": "Nobody <az8321550@gmail.com> (https://bodyno.com)",
  "license": "WTFPL",
  "dependencies": {
    "antd": "^3.1.6",
    "axios": "^0.17.1",
    "axios-jsonp": "^1.0.1",
    "debug": "^2.2.0",
    "react": "^16.4.0",
    "react-dom": "^16.4.0",
    "react-redux": "^5.0.7",
    "react-router": "^3.0.0",
    "react-spinkit": "^1.1.11",
    "redux": "^4.0.0",
    "redux-logger": "^3.0.6",
    "redux-thunk": "^2.3.0"
  },
  "devDependencies": {
    "autoprefixer": "^7.1.2",
    "babel-core": "^6.25.0",
    "babel-eslint": "^8.2.1",
    "babel-loader": "^7.1.1",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-plugin-dynamic-import-node": "^1.2.0",
    "babel-plugin-import": "^1.6.3",
    "babel-plugin-syntax-jsx": "^6.18.0",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-plugin-transform-runtime": "^6.15.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-react": "^6.16.0",
    "babel-preset-stage-2": "^6.22.0",
    "babel-runtime": "^6.11.6",
    "better-npm-run": "0.0.13",
    "classnames": "^2.2.5",
    "copy-webpack-plugin": "^4.3.1",
    "css-loader": "^0.28.0",
    "eslint": "^3.0.1",
    "eslint-config-standard": "^6.0.0",
    "eslint-config-standard-react": "^4.0.0",
    "eslint-loader": "^1.9.0",
    "eslint-plugin-babel": "^4.0.0",
    "eslint-plugin-promise": "^3.0.0",
    "eslint-plugin-react": "^6.0.0",
    "eslint-plugin-standard": "^2.0.0",
    "extract-text-webpack-plugin": "^3.0.0",
    "file-loader": "^1.1.4",
    "friendly-errors-webpack-plugin": "^1.6.1",
    "fs-extra": "^5.0.0",
    "html-webpack-plugin": "^2.22.0",
    "less": "^2.7.3",
    "less-loader": "^4.0.5",
    "node-notifier": "^5.2.1",
    "node-sass": "^4.0.0",
    "nodemon": "^1.10.2",
    "portfinder": "^1.0.13",
    "postcss-loader": "^2.0.8",
    "react-addons-test-utils": "^15.0.0",
    "react-router-redux": "^4.0.7",
    "rimraf": "^2.5.4",
    "uglifyjs-webpack-plugin": "^1.1.8",
    "url-loader": "^0.5.8",
    "webpack": "^3.10.0",
    "webpack-dev-server": "^2.11.1",
    "webpack-merge": "^4.1.1"
  }
}
